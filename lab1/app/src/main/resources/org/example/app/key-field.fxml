<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox fx:controller="org.example.app.controllers.KeyController"
      alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="642.0" spacing="5"
      xmlns="http://javafx.com/javafx/25"
      xmlns:fx="http://javafx.com/fxml" >
    <Label styleClass="info-label" text="Ключ:" HBox.hgrow="NEVER" />
    <TextField fx:id="keyTextField" promptText="Введите ключ" prefHeight="26.0" prefWidth="222.0" HBox.hgrow="NEVER" />
    <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
</HBox>